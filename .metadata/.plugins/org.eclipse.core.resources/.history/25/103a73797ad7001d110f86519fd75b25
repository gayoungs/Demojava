package quiz01;

import java.util.Scanner;

public class Quiz03 {
	
	public static void main(String[] args) {
		
		
		
	   Scanner scan = new Scanner(System.in);
		
	   int year = scan.nextInt();
	   
	   if (year % 4 == 0) {
		   if(year % 4 == 0)
			   System.out.println("1");
	   
	   else if(year % 1 == 0)
	   System.out.println("1");
	   }
	   
	   else System.out.println("0");
	  
    scan.close();
    
	}

}
