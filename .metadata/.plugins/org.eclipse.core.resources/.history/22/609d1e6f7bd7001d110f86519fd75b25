package quiz01;

import java.util.Scanner;

public class Quiz03 {
	
	public static void main(String[] args) {
		
		int a;
		int b;
		
	   Scanner scan = new Scanner(System.in);
		
	   a = scan.nextInt();
	   
	   if (a % 4 == 0 && a% 100 != 0 || a % 400 == 0) {
		   b = 1;
	   }
	   else { b = 0;
			   
	   }
	   System.out.print(b);
	 
	   if (a % 4 == 0 && a% 100 != 0 || a % 400 == 0) {
		   b = 1;
	   }
	   else { b = 0;
			   
	   }  
	   System.out.print(b);
     scan.close();
    
	}

}
